<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="io.feelfree.estimotezabytki">
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:name=".App"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.trailslist.TrailsListActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.trailbeaconlocation.TrailBeaconLocationActivity" />
        <activity android:name=".ui.quizactivity.QuizActivity" />
        <activity android:name=".ui.congratss.CongratsActivity" />
        <activity android:name=".ui.TrailEditorActivity" android:windowSoftInputMode="adjustPan"/>
        <activity android:name=".ui.beaconlist.BeaconListActivity" />
    </application>

</manifest>